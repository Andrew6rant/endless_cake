{
  "schemaVersion": 1,
  "id": "endless_cake",
  "version": "${version}",

  "name": "Endless Cake",
  "description": "A Minecraft mod for Fabric that adds an infinite cake at the cost of the dragon egg.",
  "authors": [
    "hugeblank",
    "ChocolateFrog18"
  ],
  "contact": {
    "homepage": "https://github.com/hugeblank/endless_cake",
    "sources": "https://github.com/hugeblank/endless_cake"
  },

  "license": "MIT",
  "icon": "assets/endless_cake/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "dev.elexi.hugeblank.endless_cake.Init"
    ]
  },
  "mixins": [
    "endless_cake.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.17.x",
    "java": ">=16"
  }
}
